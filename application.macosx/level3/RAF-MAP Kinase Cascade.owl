 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">MAPK/ERK kinase 1</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">ERK activator kinase 1</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">2.7.12.2</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">MAP kinase kinase 1</bp:name>
