 <bp:sequence rdf:datatype = "http://www.w3.org/2001/XMLSchema#string"></bp:sequence>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">MEK 1</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">MAP2K1</bp:name>
