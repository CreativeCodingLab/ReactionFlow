 <bp:xref rdf:resource="urn:biopax:UnificationXref:REACTOME+DATABASE+ID_83560" />
 <bp:xref rdf:resource="urn:biopax:UnificationXref:REACTOME_REACT_1809_1" />
 <bp:xref rdf:resource="http://identifiers.org/pubmed/12607005" />
